							WeaponSpecSettingsProtoSpecialSkillLightBowgunMenu = gg.choice({
								(WeaponSpecSettingsProtoSpecialSkillLightBowgun_Selected[1] and "-> [1]    体幹強化(3)" or "[1]    体幹強化(3)"),
								(WeaponSpecSettingsProtoSpecialSkillLightBowgun_Selected[2] and "-> [2]    反動軽減(3)" or "[2]    反動軽減(3)"),
