					(BattleConfigSpecialGaugeCapacity_Selected[1] and "\n[ON]    デフォルト\n" or "\n[1]    デフォルト\n"),
					(BattleConfigSpecialGaugeCapacity_Selected[2] and "\n[ON]    即フルチャージ\n" or "\n[2]    即フルチャージ\n"),
