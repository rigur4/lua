					function ArmorSpecSettingsProto_NormalSkillArm1()
						gg.getResults(9999)
						gg.refineNumber("0~7,201", gg.TYPE_DWORD, false, gg.SIGN_EQUAL, 0, -1, 0)
						gg.getResults(9999)
						gg.setVisible(false)
						local ArmorNormalSkillId = {"5801", "2403", "2305", "2503", "3503", "605", "6305", "3405", "4805", "6803", "1805", "3605"}
