								ArmorSpecSettingsProtoNormalSkillArm1_Selected[i] = false
							end
							ArmorSpecSettingsProtoNormalSkillArm1_Selected[ArmorSpecSettingsProtoNormalSkillArm1Menu] = true				
							if ArmorSpecSettingsProtoNormalSkillArm1Menu >= 1 and ArmorSpecSettingsProtoNormalSkillArm1Menu <= 12 then
								local chosenValue = ArmorNormalSkillId[ArmorSpecSettingsProtoNormalSkillArm1Menu]
								gg.editAll(chosenValue, gg.TYPE_DWORD)
								ArmorSpecSettingsProto_NormalSkillArm1()
							elseif ArmorSpecSettingsProtoNormalSkillArm1Menu == 13 then
