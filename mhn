								(WeaponSpecSettingsProtoSpecialSkillLightBowgun_Selected[5] and "-> [5]    睡眠耐性(5)" or "[5]    睡眠耐性(5)"),
								(WeaponSpecSettingsProtoSpecialSkillLightBowgun_Selected[6] and "-> [6]    耳栓(5)" or "[6]    耳栓(5)"),
