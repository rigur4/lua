						(WeaponLevelSpecSettingsProtoCritical_Selected[3] and "-> [3]    " .. WeaponCriticalValue[3] or "[3]    " .. WeaponCriticalValue[3]),
						(WeaponLevelSpecSettingsProtoCritical_Selected[4] and "-> [4]    任意の値" or "[4]    任意の値"),
						"\n[←]    もどる"
