							gg.refineNumber("5", gg.TYPE_DWORD, false, gg.SIGN_EQUAL, 0, -1, 0)
							applyoffset(offset[2])
							MoveThePointer()
							applyoffset(offset[3])
							MoveThePointer()
							applyoffset(offset[6])
							MoveThePointer()
							applyoffset(offset[7])
							refine()
							ArmorSpecSettingsProto_NormalSkillLegs2()								
						else
							ArmorSpecSettingsProto_NormalSkillsetting()
						end
					elseif ArmorSelectNormalSkillMenu == 12 then
						ArmorSpec()
					end				
				end
					function ArmorSpecSettingsProto_NormalSkillHead1()
						gg.getResults(9999)
