   Version : 68.2
UpdateDate : 2024/1/20
]]--
userMode = 2
gg.clearList()
gg.clearResults()
gg.alert("少しずつコードを追加させていく予定です。\n2024/1/20\nかなり大変でしたが、装備項目ができました。\n何か問題があれば教えてください")
---------------------------------------------------------------------------
UserHunterWeaponWeaponLevel_Selected = {false, false, false}
UserHunterArmorArmorLevel_Selected = {false, false, false}
SeriesSpecSettingsProtoNumRequiredArmor_Selected = {false, false}
---------------------------------------------------------------------------
WeaponLevelSpecSettingsProtoAttack_Selected = {false, false, false, false}
WeaponLevelSpecSettingsProtoElementAttack_Selected = {false, false, false, false}
WeaponLevelSpecSettingsProtoCritical_Selected = {false, false, false, false}
WeaponSpecSettingsProtoElementType_Selected = {false, false, false, false, false, false, false, false, false, false, false}
WeaponSpecSettingsProtoNormalSkillsetting_Selected = {false, false, false, false, false, false, false, false, false}
WeaponSpecSettingsProtoNormalSkillAll_Selected = {false, false, false, false, false, false, false, false, false, false, false, false, false}
WeaponSpecSettingsProtoNormalSkillGreatSword_Selected = {false, false, false, false, false, false, false, false, false, false, false}
WeaponSpecSettingsProtoNormalSkillSwordShield_Selected = {false, false, false, false, false, false, false, false, false, false}
WeaponSpecSettingsProtoNormalSkillDualBlades_Selected = {false, false, false, false, false, false, false, false, false, false}
WeaponSpecSettingsProtoNormalSkillLongSword_Selected = {false, false, false, false, false, false, false, false, false, false, false}
WeaponSpecSettingsProtoNormalSkillHammer_Selected = {false, false, false, false, false, false, false, false, false, false}
WeaponSpecSettingsProtoNormalSkillLance_Selected = {false, false, false, false, false, false, false, false, false, false}
WeaponSpecSettingsProtoNormalSkillBow_Selected = {false, false, false, false, false, false, false, false, false, false, false}
WeaponSpecSettingsProtoNormalSkillLightBowgun_Selected = {false, false, false, false, false, false, false, false, false, false}
WeaponSpecSettingsProtoSpecialSkill_AllSelected = {false, false, false, false}
WeaponSpecSettingsProtoSpecialSkillsetting_Selected = {false, false, false, false, false, false, false, false, false}
WeaponSpecSettingsPrototoNormalSkillAll_Selected = {false, false, false, false, false, false, false, false, false, false, false, false, false}
WeaponSpecSettingsPrototoNormalSkillGreatSword_Selected = {false, false, false, false, false, false, false, false, false, false, false}
WeaponSpecSettingsPrototoNormalSkillSwordShield_Selected = {false, false, false, false, false, false, false, false, false, false}
WeaponSpecSettingsPrototoNormalSkillDualBlades_Selected = {false, false, false, false, false, false, false, false, false, false}
WeaponSpecSettingsPrototoNormalSkillLongSword_Selected = {false, false, false, false, false, false, false, false, false, false, false}
WeaponSpecSettingsPrototoNormalSkillHammer_Selected = {false, false, false, false, false, false, false, false, false, false}
WeaponSpecSettingsPrototoNormalSkillLance_Selected = {false, false, false, false, false, false, false, false, false, false}
WeaponSpecSettingsPrototoNormalSkillBow_Selected = {false, false, false, false, false, false, false, false, false, false}
WeaponSpecSettingsProtoSpecialSkillLightBowgun_Selected = {false, false, false, false, false, false, false, false, false, false, false}
---------------------------------------------------------------------------
ArmorLevelSpecSettingsProtoDefence_Selected = {false, false, false, false}
ArmorLevelSpecSettingsProtoElementDefence_Selected = {false, false, false, false}
ArmorSpecSettingsProtoElementType_Selected = {false, false, false, false, false, false, false, false, false, false, false}
ArmorSpecSettingsProtoNormalSkillsetting_Selected = {false, false, false, false, false, false, false, false, false, false, false}
ArmorSpecSettingsProtoNormalSkillHead1_Selected = {false, false, false, false, false, false, false, false, false, false, false, false, false}
ArmorSpecSettingsProtoNormalSkillHead2_Selected = {false, false, false, false, false, false, false, false, false, false, false, false, false}
ArmorSpecSettingsProtoNormalSkillChest1_Selected = {false, false, false, false, false, false, false, false, false, false, false, false, false}
ArmorSpecSettingsProtoNormalSkillChest2_Selected = {false, false, false, false, false, false, false, false, false, false, false, false, false}
ArmorSpecSettingsProtoNormalSkillArm1_Selected = {false, false, false, false, false, false, false, false, false, false, false, false, false}
ArmorSpecSettingsProtoNormalSkillArm2_Selected = {false, false, false, false, false, false, false, false, false, false, false, false, false}
ArmorSpecSettingsProtoNormalSkillTorso1_Selected = {false, false, false, false, false, false, false, false, false, false, false, false, false}
ArmorSpecSettingsProtoNormalSkillTorso2_Selected = {false, false, false, false, false, false, false, false, false, false, false, false, false}
ArmorSpecSettingsProtoNormalSkillLegs1_Selected = {false, false, false, false, false, false, false, false, false, false, false, false, false}
ArmorSpecSettingsProtoNormalSkillLegs2_Selected = {false, false, false, false, false, false, false, false, false, false, false, false, false}
