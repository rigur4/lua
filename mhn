			Bowgun_SelectedB = {"\n[2]   装填パターン*\n", "\n[ON]   装填パターン*  =  小\n", "\n[ON]   装填パターン*  =  中\n", "\n[ON]   装填パターン*  =  大\n", "\n[ON]   装填パターン*  =  特大\n"}					
