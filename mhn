								WeaponSpecSettingsProto_SpecialSkillsetting()
							end
						end	
						function WeaponSpecSettingsProto_toNormalSkillBow()
							gg.getResults(9999)
							gg.refineNumber("0~7,201", gg.TYPE_DWORD, false, gg.SIGN_EQUAL, 0, -1, 0)
							gg.getResults(9999)
							gg.setVisible(false)
							local WeapontoNormalSkillId = {"2403", "2305", "605", "6305", "3405", "4805", "6803", "1805", "3605"}
							WeaponSpecSettingsPrototoNormalSkillBowMenu = gg.choice({
								(WeaponSpecSettingsPrototoNormalSkillBow_Selected[1] and "-> [1]    体幹強化(3)" or "[1]    体幹強化(3)"),
								(WeaponSpecSettingsPrototoNormalSkillBow_Selected[2] and "-> [2]    集中(5)" or "[2]    集中(5)"),
								(WeaponSpecSettingsPrototoNormalSkillBow_Selected[3] and "-> [3]    麻痺耐性(5)" or "[3]    麻痺耐性(5)"),
								(WeaponSpecSettingsPrototoNormalSkillBow_Selected[4] and "-> [4]    睡眠耐性(5)" or "[4]    睡眠耐性(5)"),
								(WeaponSpecSettingsPrototoNormalSkillBow_Selected[5] and "-> [5]    耳栓(5)" or "[5]    耳栓(5)"),
								(WeaponSpecSettingsPrototoNormalSkillBow_Selected[6] and "-> [6]    風圧耐性(5)" or "[6]    風圧耐性(5)"),
								(WeaponSpecSettingsPrototoNormalSkillBow_Selected[7] and "-> [7]    耐震(3)" or "[7]    耐震(3)"),
								(WeaponSpecSettingsPrototoNormalSkillBow_Selected[8] and "-> [8]    回避距離UP(5)" or "[8]    回避距離UP(5)"),
								(WeaponSpecSettingsPrototoNormalSkillBow_Selected[9] and "-> [9]    ジャスト回避強化(5)" or "[9]    ジャスト回避強化(5)"),
								(WeaponSpecSettingsPrototoNormalSkillBow_Selected[10] and "-> [10]    任意の値" or "[10]    任意の値"),
								"\n[←]    もどる"
							}, nil, "メインメニュー/装備/武器/全武器の性能/スペシャルスキル/ノーマルスキルに変更(武器の選択)/弓")
							if WeaponSpecSettingsPrototoNormalSkillBowMenu == nil then
								temporarilyhide()
								WeaponSpecSettingsProto_toNormalSkillBow()
							elseif WeaponSpecSettingsPrototoNormalSkillBowMenu >= 1 and WeaponSpecSettingsPrototoNormalSkillBowMenu <= 10 then
								for i = 1, 10 do
									WeaponSpecSettingsPrototoNormalSkillBow_Selected[i] = false
								end
								WeaponSpecSettingsPrototoNormalSkillBow_Selected[WeaponSpecSettingsPrototoNormalSkillBowMenu] = true				
								if WeaponSpecSettingsPrototoNormalSkillBowMenu >= 1 and WeaponSpecSettingsPrototoNormalSkillBowMenu <= 9 then
									local chosenValue = WeapontoNormalSkillId[WeaponSpecSettingsPrototoNormalSkillBowMenu]
									gg.editAll(chosenValue, gg.TYPE_DWORD)
									WeaponSpecSettingsProto_toNormalSkillBow()
								elseif WeaponSpecSettingsPrototoNormalSkillBowMenu == 10 then
									while true do
										gg.setVisible(false)
										val = gg.prompt({[1] = "弓のノーマルスキルを変更\n以下を参照し1つを入力\nノーマルスキルID:\n体力増強(5):105, 攻撃(5):205, 防御(5):305, SPゲージ加速(5):405, \n毒耐性(5):505, 麻痺耐性(5):605, 火耐性(5):705, 水耐性(5):805, \n氷耐性(5):905, 雷耐性(5):1005, 毒属性強化(5):1105, \n麻痺属性強化(5):1205, 炎属性強化(5):1305, 水属性強化(5):1405, \n氷属性強化(5):1505, 雷属性強化(5):1605, 見切り(5):1705,\n回避距離UP(5):1805, 弱点特攻(5):1905, 破壊王(5):2005, \n精霊の加護(5):2205, 集中(5):2305, 体幹強化(3):2403, \n反動軽減(3):2503, 根性(5):2605, 攻めの守勢(5):2805, 連撃(5):3005, \nガード性能(5):3105, 背水防御(5):3205, 火事場力(5):3305, 耳栓(5):3405, \n装填速度(3):3503, ジャスト回避強化(5):3605, 尻上がり(5):3705, \n貫通弾強化(3):3803, 徹甲榴弾強化(3):3903, 斬裂弾強化(3):4003, \n散弾強化(3):4103, 闇討ち(5):4205, フルチャージ(5):4305, 超会心(5):4505, \n不屈(5):4605, SPスキル威力アップ(5):4705, 風圧耐性(5):4805, \nKO術(5):4905, 逆恨み(5):5605, ジャスト回避強化[仮](5):5705, \nロックオン(1):5801, 龍属性攻撃強化(5):5905, 団結力【秋のかぼちゃ狩り】(1):6001, \nジャスト巧攻(5):6105, 睡眠攻撃強化(5):6205, 睡眠耐性(5):6305, \nSPゲージ加速【ジャスト回避】(5):6405, SPゲージ加速【ガード】(5):6505, \n軽巧(5):6605, 進撃(5):6705, 耐震(3):6803, ハッピーニューイヤー【2024】(1):7201, なし:範囲外の値"}, {[1] = ""})
										if val and val[1] ~= nil then
											gg.editAll(val[1], gg.TYPE_DWORD)									
											break
										else
											temporarilyhide()
										end
									end
									WeaponSpecSettingsProto_toNormalSkillBow()
								end
							elseif WeaponSpecSettingsPrototoNormalSkillBowMenu == 11 then
								WeaponSpecSettingsProto_SpecialSkillsetting()
							end
						end
