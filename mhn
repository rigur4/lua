				function ArmorSpecSettingsProto_NormalSkillsetting()
					gg.clearResults()
					gg.loadResults(gg.getListItems())
					local offset = {0x18, 0x20, 0x10, 0x20, 0x10,0x28, 0x10}
        			local function applyoffset(selectedOffset)
        				local results = gg.getResults(9999)
        				gg.clearResults()
        				for i, v in ipairs(results) do
        					v.address = v.address + selectedOffset
        					v.flags = gg.TYPE_DWORD
        				end
        			    gg.loadResults(results)
        			end					
					local function refine()
						local ranges = {{"5000~5503"},{"7000~7103"}}
						gg.getResults(9999)
						for _, rangeList in ipairs(ranges) do
							for _, range in ipairs(rangeList) do
								gg.getResults(9999)
								gg.refineNumber(range, gg.TYPE_DWORD, false, gg.SIGN_NOT_EQUAL, 0, -1, 0)
							end
						end
					end
					gg.setVisible(false)
					local ArmorSelectNormalSkillMenu = gg.choice({
						(ArmorSpecSettingsProtoNormalSkillsetting_Selected[1] and "-> [1]    全身防具(お気に入り変更)" or "[1]    全身防具(お気に入り変更)"),
						(ArmorSpecSettingsProtoNormalSkillsetting_Selected[2] and "-> [2]    頭1" or "[2]    頭1"),
						(ArmorSpecSettingsProtoNormalSkillsetting_Selected[3] and "-> [3]    頭2" or "[3]    頭2"),
						(ArmorSpecSettingsProtoNormalSkillsetting_Selected[4] and "-> [4]    胴1" or "[4]    胴1"),
						(ArmorSpecSettingsProtoNormalSkillsetting_Selected[5] and "-> [5]    胴2" or "[5]    胴2"),
						(ArmorSpecSettingsProtoNormalSkillsetting_Selected[6] and "-> [6]    腕1" or "[6]    腕1"),
						(ArmorSpecSettingsProtoNormalSkillsetting_Selected[7] and "-> [7]    腕2" or "[7]    腕2"),
						(ArmorSpecSettingsProtoNormalSkillsetting_Selected[8] and "-> [8]    腰1" or "[8]    腰1"),
						(ArmorSpecSettingsProtoNormalSkillsetting_Selected[9] and "-> [9]    腰2" or "[9]    腰2"),
						(ArmorSpecSettingsProtoNormalSkillsetting_Selected[10] and "-> [10]    脚1" or "[10]    脚1"),
						(ArmorSpecSettingsProtoNormalSkillsetting_Selected[11] and "-> [11]    脚2" or "[11]    脚2"),
						"\n[←]    もどる"
					}, nil, "メインメニュー/装備/防具/全防具の性能/ノーマルスキル(防具の選択)")
					if ArmorSelectNormalSkillMenu == nil then
						temporarilyhide()
						ArmorSpecSettingsProto_NormalSkillsetting()
					elseif ArmorSelectNormalSkillMenu >= 1 and ArmorSelectNormalSkillMenu <= 11 then	
						ArmorSpecSettingsProtoNormalSkillsetting_Selected[ArmorSelectNormalSkillMenu] = true	
						if ArmorSelectNormalSkillMenu == 1 then
							ArmorSpecSettingsProtoNormalSkillsetting_Selected = {true, false, false, false, false, false, false, false, false, false, false}
							local promptResult = gg.prompt({[1] = "全身防具(お気に入り変更)スキル変更\n\n内容:(ロックオン1;集中5;反動軽減3;装填速度3) +\n体幹強化5,麻痺耐性5,睡眠耐性5,耳栓5,風圧耐性5,耐震3,回避距離UP5,ジャスト回避強化5\n<以下から1つを入力>\n大剣用:1, \n片手剣・双剣・ハンマー・ランス用:2, \n弓用:3, \nライトボウガン用:4"}, {[1] = ""})
							if promptResult == nil then
								gg.alert("有効な値を入力してください")
								ArmorSpecSettingsProto_NormalSkillsetting()
							end
							local WeaponChoice = tonumber(promptResult[1])
							if WeaponChoice == 1 then
								applyoffset(offset[1])
								gg.refineNumber("1", gg.TYPE_DWORD, false, gg.SIGN_EQUAL, 0, -1, 0)
								applyoffset(offset[2])
								MoveThePointer()
								applyoffset(offset[3])
								MoveThePointer()
								applyoffset(offset[4])
								MoveThePointer()
								applyoffset(offset[5])
								gg.getResults(9999)
								gg.editAll("2305",gg.TYPE_DWORD)
								gg.clearResults()
								gg.loadResults(gg.getListItems())							
								applyoffset(offset[1])
								gg.refineNumber("1", gg.TYPE_DWORD, false, gg.SIGN_EQUAL, 0, -1, 0)
								applyoffset(offset[2])
								MoveThePointer()
								applyoffset(offset[3])
								MoveThePointer()
								applyoffset(offset[6])
								MoveThePointer()
								applyoffset(offset[7])
								gg.getResults(9999)
								gg.editAll("5801",gg.TYPE_DWORD)							
								gg.clearResults()
								gg.loadResults(gg.getListItems())							
								applyoffset(offset[1])
								gg.refineNumber("2", gg.TYPE_DWORD, false, gg.SIGN_EQUAL, 0, -1, 0)
								applyoffset(offset[2])
								MoveThePointer()
								applyoffset(offset[3])
								MoveThePointer()
								applyoffset(offset[4])
								MoveThePointer()
								applyoffset(offset[5])
								gg.getResults(9999)
								gg.editAll("2403",gg.TYPE_DWORD)	
								gg.clearResults()
								gg.loadResults(gg.getListItems())							
								applyoffset(offset[1])
								gg.refineNumber("2", gg.TYPE_DWORD, false, gg.SIGN_EQUAL, 0, -1, 0)
								applyoffset(offset[2])
								MoveThePointer()
								applyoffset(offset[3])
								MoveThePointer()
								applyoffset(offset[6])
								MoveThePointer()
								applyoffset(offset[7])
								gg.getResults(9999)
								gg.editAll("605",gg.TYPE_DWORD)	
								gg.clearResults()
								gg.loadResults(gg.getListItems())							
								applyoffset(offset[1])
								gg.refineNumber("3", gg.TYPE_DWORD, false, gg.SIGN_EQUAL, 0, -1, 0)
								applyoffset(offset[2])
								MoveThePointer()
								applyoffset(offset[3])
								MoveThePointer()
								applyoffset(offset[4])
								MoveThePointer()
								applyoffset(offset[5])
								gg.getResults(9999)
								gg.editAll("6305",gg.TYPE_DWORD)	
								gg.clearResults()
								gg.loadResults(gg.getListItems())							
								applyoffset(offset[1])
