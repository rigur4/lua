									gg.editAll(val[1], gg.TYPE_DWORD)									
									break
								else
									temporarilyhide()
								end
							end
							WeaponLevelSpecSettingsProto_ElementAttack()
						end
					elseif WeaponLevelSpecSettingsProtoElementAttackMenu == 5 then
