						local value2 = userInput[2]
						local value3 = userInput[3]	
						gg.editAll(value1, gg.TYPE_FLOAT)
						applyoffset(0x4, gg.TYPE_FLOAT)
						gg.editAll(value2, gg.TYPE_FLOAT)
						applyoffset(0x4, gg.TYPE_DWORD)
						gg.editAll(value3, gg.TYPE_DWORD)							
					end
