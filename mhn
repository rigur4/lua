				function WeaponSpecSettingsProto_SpecialSkillAll()
					gg.clearResults()
					gg.loadResults(gg.getListItems())
					local offset = {0x18, 0x28, 0x10}
					local function applyoffset(selectedOffset)
						local results = gg.getResults(9999)
						gg.clearResults()
						for i, v in ipairs(results) do
							v.address = v.address + selectedOffset
							v.flags = gg.TYPE_DWORD
						end
						gg.loadResults(results)
					end
					gg.setVisible(false)
					local WeaponSpecialSkillAllMenu = gg.choice({
						(WeaponSpecSettingsProtoSpecialSkill_AllSelected[1] and "-> [1]    Lv1" or "[1]    Lv1"),
						(WeaponSpecSettingsProtoSpecialSkill_AllSelected[2] and "-> [2]    Lv2" or "[2]    Lv2"),
						(WeaponSpecSettingsProtoSpecialSkill_AllSelected[3] and "-> [3]    Lv3" or "[3]    Lv3"),
						(WeaponSpecSettingsProtoSpecialSkill_AllSelected[4] and "\n-> [4]    ノーマルスキルに変更" or "\n[4]    ノーマルスキルに変更"),
						"\n[←]    もどる"
					}, nil, "メインメニュー/装備/武器/全武器の性能/スペシャルスキル")
					if WeaponSpecialSkillAllMenu == nil then
						temporarilyhide()
						WeaponSpecSettingsProto_SpecialSkillAll()
					elseif WeaponSpecialSkillAllMenu >= 1 and WeaponSpecialSkillAllMenu <= 4 then	
						for i = 1, 4 do
							WeaponSpecSettingsProtoSpecialSkill_AllSelected[i] = false
						end
						WeaponSpecSettingsProtoSpecialSkill_AllSelected[WeaponSpecialSkillAllMenu] = true	
						if WeaponSpecialSkillAllMenu >= 1 and WeaponSpecialSkillAllMenu <= 3 then							
							local function processNumber(number, resultValue)
								gg.clearResults()
								gg.loadResults(gg.getListItems())
								applyoffset(offset[1])
								gg.getResults(9999)
								gg.refineNumber(tostring(number), gg.TYPE_DWORD, false, gg.SIGN_EQUAL, 0, -1, 0)  -- Refine the number
								applyoffset(offset[2])
								MoveThePointer()
								applyoffset(offset[3])
								gg.getResults(9999)
								gg.editAll(tostring(resultValue), gg.TYPE_DWORD)
							end
							if WeaponSpecialSkillAllMenu == 1 then
								processNumber(1, 5001)
								processNumber(2, 5101)
								processNumber(3, 7001)
								processNumber(4, 5201)
								processNumber(5, 5301)
								processNumber(7, 7101)
								processNumber(12, 5501)
								processNumber(14, 5401)
								WeaponSpecSettingsProto_SpecialSkillAll()	
							elseif WeaponSpecialSkillAllMenu == 2 then
								processNumber(1, 5002)
								processNumber(2, 5102)
								processNumber(3, 7002)
								processNumber(4, 5202)
								processNumber(5, 5302)
								processNumber(7, 7102)
								processNumber(12, 5502)
								processNumber(14, 5402)	
								WeaponSpecSettingsProto_SpecialSkillAll()
							elseif WeaponSpecialSkillAllMenu == 3 then	
								processNumber(1, 5003)
								processNumber(2, 5103)
								processNumber(3, 7003)
								processNumber(4, 5203)
								processNumber(5, 5303)
								processNumber(7, 7103)
								processNumber(12, 5503)
								processNumber(14, 5403)
								WeaponSpecSettingsProto_SpecialSkillAll()
							end
						elseif WeaponSpecialSkillAllMenu == 4 then
							WeaponSpecSettingsProto_SpecialSkillsetting()
						end
					elseif WeaponSpecialSkillAllMenu == 5 then
						WeaponSpec()
					end
				end		
					function WeaponSpecSettingsProto_SpecialSkillsetting()
						gg.clearResults()
						gg.loadResults(gg.getListItems())
						local offset = {0x18, 0x28, 0x10}
						local function applyoffset(selectedOffset)
						local results = gg.getResults(9999)
							gg.clearResults()
							for i, v in ipairs(results) do
								v.address = v.address + selectedOffset
								v.flags = gg.TYPE_DWORD
							end
							gg.loadResults(results)
						end					
						gg.setVisible(false)
						local WeaponSelectSpecialSkillMenu = gg.choice({
							(WeaponSpecSettingsProtoSpecialSkillsetting_Selected[1] and "-> [1]    全武器(一括変更)" or "[1]    全武器(一括変更)"),
							(WeaponSpecSettingsProtoSpecialSkillsetting_Selected[2] and "-> [2]    大剣" or "[2]    大剣"),
							(WeaponSpecSettingsProtoSpecialSkillsetting_Selected[3] and "-> [3]    片手剣" or "[3]    片手剣"),
							(WeaponSpecSettingsProtoSpecialSkillsetting_Selected[4] and "-> [4]    双剣" or "[4]    双剣"),
							(WeaponSpecSettingsProtoSpecialSkillsetting_Selected[5] and "-> [5]    太刀" or "[5]    太刀"),
							(WeaponSpecSettingsProtoSpecialSkillsetting_Selected[6] and "-> [6]    ハンマー" or "[6]    ハンマー"),
							(WeaponSpecSettingsProtoSpecialSkillsetting_Selected[7] and "-> [7]    ランス" or "[7]    ランス"),
							(WeaponSpecSettingsProtoSpecialSkillsetting_Selected[8] and "-> [8]    弓" or "[8]    弓"),
							(WeaponSpecSettingsProtoSpecialSkillsetting_Selected[9] and "-> [9]    ライトボウガン" or "[9]    ライトボウガン"),
							"\n[←]    もどる"
						}, nil, "メインメニュー/装備/武器/全武器の性能/スペシャルスキル/ノーマルスキルに変更(武器の選択)")
						if WeaponSelectSpecialSkillMenu == nil then
							temporarilyhide()
							WeaponSpecSettingsProto_SpecialSkillsetting()
						elseif WeaponSelectSpecialSkillMenu >= 1 and WeaponSelectSpecialSkillMenu <= 9 then	
							WeaponSpecSettingsProtoSpecialSkillsetting_Selected[WeaponSelectSpecialSkillMenu] = true	
							if WeaponSelectSpecialSkillMenu == 1 then				
								WeaponSpecSettingsProtoSpecialSkillsetting_Selected = {true, false, false, false, false, false, false, false, false}
								applyoffset(offset[1])
								applyoffset(offset[2])
								MoveThePointer()
								applyoffset(offset[3])
								WeaponSpecSettingsProto_toNormalSkillAll()                            
							elseif WeaponSelectSpecialSkillMenu == 2 then
								WeaponSpecSettingsProtoSpecialSkillsetting_Selected[1] = false
								applyoffset(offset[1])
								gg.refineNumber("1", gg.TYPE_DWORD, false, gg.SIGN_EQUAL, 0, -1, 0)
								applyoffset(offset[2])
								MoveThePointer()
								applyoffset(offset[3])
								WeaponSpecSettingsProto_toNormalSkillGreatSword()
							elseif WeaponSelectSpecialSkillMenu == 3 then
								WeaponSpecSettingsProtoSpecialSkillsetting_Selected[1] = false
								applyoffset(offset[1])
								gg.refineNumber("2", gg.TYPE_DWORD, false, gg.SIGN_EQUAL, 0, -1, 0)
								applyoffset(offset[2])
								MoveThePointer()
								applyoffset(offset[3])
								WeaponSpecSettingsProto_toNormalSkillSwordShield()								
							elseif WeaponSelectSpecialSkillMenu == 4 then
								WeaponSpecSettingsProtoSpecialSkillsetting_Selected[1] = false
								applyoffset(offset[1])
								gg.refineNumber("3", gg.TYPE_DWORD, false, gg.SIGN_EQUAL, 0, -1, 0)
								applyoffset(offset[2])
								MoveThePointer()
								applyoffset(offset[3])
								WeaponSpecSettingsProto_toNormalSkillDualBlades()								
							elseif WeaponSelectSpecialSkillMenu == 5 then
								WeaponSpecSettingsProtoSpecialSkillsetting_Selected[1] = false
								applyoffset(offset[1])
								gg.refineNumber("4", gg.TYPE_DWORD, false, gg.SIGN_EQUAL, 0, -1, 0)
								applyoffset(offset[2])
								MoveThePointer()
								applyoffset(offset[3])
								WeaponSpecSettingsProto_toNormalSkillLongSword()							
							elseif WeaponSelectSpecialSkillMenu == 6 then
								WeaponSpecSettingsProtoSpecialSkillsetting_Selected[1] = false
								applyoffset(offset[1])
								gg.refineNumber("5", gg.TYPE_DWORD, false, gg.SIGN_EQUAL, 0, -1, 0)
								applyoffset(offset[2])
								MoveThePointer()
								applyoffset(offset[3])
								WeaponSpecSettingsProto_toNormalSkillHammer()								
							elseif WeaponSelectSpecialSkillMenu == 7 then	
								WeaponSpecSettingsProtoSpecialSkillsetting_Selected[1] = false
								applyoffset(offset[1])
								gg.refineNumber("7", gg.TYPE_DWORD, false, gg.SIGN_EQUAL, 0, -1, 0)
								applyoffset(offset[2])
								MoveThePointer()
								applyoffset(offset[3])
								WeaponSpecSettingsProto_toNormalSkillLance()								
							elseif WeaponSelectSpecialSkillMenu == 8 then
								WeaponSpecSettingsProtoSpecialSkillsetting_Selected[1] = false
								applyoffset(offset[1])
								gg.refineNumber("12", gg.TYPE_DWORD, false, gg.SIGN_EQUAL, 0, -1, 0)
								applyoffset(offset[2])
								MoveThePointer()
								applyoffset(offset[3])
								WeaponSpecSettingsProto_toNormalSkillBow()								
							elseif WeaponSelectSpecialSkillMenu == 9 then
								WeaponSpecSettingsProtoSpecialSkillsetting_Selected[1] = false
								applyoffset(offset[1])
								gg.refineNumber("14", gg.TYPE_DWORD, false, gg.SIGN_EQUAL, 0, -1, 0)
								applyoffset(offset[2])
								MoveThePointer()
								applyoffset(offset[3])
								WeaponSpecSettingsProto_toNormalSkillLightBowgun()							
							else
								WeaponSpecSettingsProto_SpecialSkillsetting()
							end
						elseif WeaponSelectSpecialSkillMenu == 10 then
							WeaponSpecSettingsProto_SpecialSkillAll()
						end				
					end
						function WeaponSpecSettingsProto_toNormalSkillAll()
							gg.getResults(9999)
							gg.refineNumber("0~7,201", gg.TYPE_DWORD, false, gg.SIGN_EQUAL, 0, -1, 0)
							gg.getResults(9999)
							gg.setVisible(false)
							local WeapontoNormalSkillId = {"5801", "2403", "2305", "2503", "3503", "605", "6305", "3405", "4805", "6803", "1805", "3605"}
							WeaponSpecSettingsPrototoNormalSkillAllMenu = gg.choice({
								(WeaponSpecSettingsPrototoNormalSkillAll_Selected[1] and "-> [1]    ロックオン(1)" or "[1]    ロックオン(1)"),
								(WeaponSpecSettingsPrototoNormalSkillAll_Selected[2] and "-> [2]    体幹強化(3)" or "[2]    体幹強化(3)"),
								(WeaponSpecSettingsPrototoNormalSkillAll_Selected[3] and "-> [3]    集中(5)" or "[3]    集中(5)"),
								(WeaponSpecSettingsPrototoNormalSkillAll_Selected[4] and "-> [4]    反動軽減(3)" or "[4]    反動軽減(3)"),
								(WeaponSpecSettingsPrototoNormalSkillAll_Selected[5] and "-> [5]    装填速度(3)" or "[5]    装填速度(3)"),
								(WeaponSpecSettingsPrototoNormalSkillAll_Selected[6] and "-> [6]    麻痺耐性(5)" or "[6]    麻痺耐性(5)"),
								(WeaponSpecSettingsPrototoNormalSkillAll_Selected[7] and "-> [7]    睡眠耐性(5)" or "[7]    睡眠耐性(5)"),
								(WeaponSpecSettingsPrototoNormalSkillAll_Selected[8] and "-> [8]    耳栓(5)" or "[8]    耳栓(5)"),
								(WeaponSpecSettingsPrototoNormalSkillAll_Selected[9] and "-> [9]    風圧耐性(5)" or "[9]    風圧耐性(5)"),
								(WeaponSpecSettingsPrototoNormalSkillAll_Selected[10] and "-> [10]    耐震(3)" or "[10]    耐震(3)"),
								(WeaponSpecSettingsPrototoNormalSkillAll_Selected[11] and "-> [11]    回避距離UP(5)" or "[11]    回避距離UP(5)"),
								(WeaponSpecSettingsPrototoNormalSkillAll_Selected[12] and "-> [12]    ジャスト回避強化(5)" or "[12]    ジャスト回避強化(5)"),
								(WeaponSpecSettingsPrototoNormalSkillAll_Selected[13] and "-> [13]    任意の値" or "[13]    任意の値"),
								"\n[←]    もどる"
							}, nil, "メインメニュー/装備/武器/全武器の性能/スペシャルスキル/ノーマルスキルに変更(武器の選択)/全武器(一括変更)")
							if WeaponSpecSettingsPrototoNormalSkillAllMenu == nil then
								temporarilyhide()
								WeaponSpecSettingsProto_toNormalSkillAll()
							elseif WeaponSpecSettingsPrototoNormalSkillAllMenu >= 1 and WeaponSpecSettingsPrototoNormalSkillAllMenu <= 13 then
								for i = 1, 13 do
									WeaponSpecSettingsPrototoNormalSkillAll_Selected[i] = false
								end
								WeaponSpecSettingsPrototoNormalSkillAll_Selected[WeaponSpecSettingsPrototoNormalSkillAllMenu] = true				
								if WeaponSpecSettingsPrototoNormalSkillAllMenu >= 1 and WeaponSpecSettingsPrototoNormalSkillAllMenu <= 12 then
									local chosenValue = WeapontoNormalSkillId[WeaponSpecSettingsPrototoNormalSkillAllMenu]
									gg.editAll(chosenValue, gg.TYPE_DWORD)
									WeaponSpecSettingsProto_toNormalSkillAll()
								elseif WeaponSpecSettingsPrototoNormalSkillAllMenu == 13 then
									while true do
										gg.setVisible(false)
										val = gg.prompt({[1] = "全武器のノーマルスキルを変更\n以下を参照し1つを入力\nノーマルスキルID:\n体力増強(5):105, 攻撃(5):205, 防御(5):305, SPゲージ加速(5):405, \n毒耐性(5):505, 麻痺耐性(5):605, 火耐性(5):705, 水耐性(5):805, \n氷耐性(5):905, 雷耐性(5):1005, 毒属性強化(5):1105, \n麻痺属性強化(5):1205, 炎属性強化(5):1305, 水属性強化(5):1405, \n氷属性強化(5):1505, 雷属性強化(5):1605, 見切り(5):1705,\n回避距離UP(5):1805, 弱点特攻(5):1905, 破壊王(5):2005, \n精霊の加護(5):2205, 集中(5):2305, 体幹強化(3):2403, \n反動軽減(3):2503, 根性(5):2605, 攻めの守勢(5):2805, 連撃(5):3005, \nガード性能(5):3105, 背水防御(5):3205, 火事場力(5):3305, 耳栓(5):3405, \n装填速度(3):3503, ジャスト回避強化(5):3605, 尻上がり(5):3705, \n貫通弾強化(3):3803, 徹甲榴弾強化(3):3903, 斬裂弾強化(3):4003, \n散弾強化(3):4103, 闇討ち(5):4205, フルチャージ(5):4305, 超会心(5):4505, \n不屈(5):4605, SPスキル威力アップ(5):4705, 風圧耐性(5):4805, \nKO術(5):4905, 逆恨み(5):5605, ジャスト回避強化[仮](5):5705, \nロックオン(1):5801, 龍属性攻撃強化(5):5905, 団結力【秋のかぼちゃ狩り】(1):6001, \nジャスト巧攻(5):6105, 睡眠攻撃強化(5):6205, 睡眠耐性(5):6305, \nSPゲージ加速【ジャスト回避】(5):6405, SPゲージ加速【ガード】(5):6505, \n軽巧(5):6605, 進撃(5):6705, 耐震(3):6803, ハッピーニューイヤー【2024】(1):7201, なし:範囲外の値"}, {[1] = ""})
										if val and val[1] ~= nil then
											gg.editAll(val[1], gg.TYPE_DWORD)									
											break
										else
											temporarilyhide()
										end
									end
									WeaponSpecSettingsProto_toNormalSkillAll()
								end
							elseif WeaponSpecSettingsPrototoNormalSkillAllMenu == 14 then
								WeaponSpecSettingsProto_SpecialSkillsetting()
							end
						end	
