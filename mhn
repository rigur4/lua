								if val and val[1] ~= nil then
									gg.editAll(val[1], gg.TYPE_DWORD)
									break
								else
									temporarilyhide()
								end
							end		
							ArrowSpec_ArrowLevel()
						end
					elseif ArrowSpecArrowLvelMenu == 6 then
