							for i = 1, 13 do
								ArmorSpecSettingsProtoNormalSkillTorso2_Selected[i] = false
							end
							ArmorSpecSettingsProtoNormalSkillTorso2_Selected[ArmorSpecSettingsProtoNormalSkillTorso2Menu] = true				
