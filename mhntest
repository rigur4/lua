							while true do
								gg.setVisible(false)
								val = gg.prompt({[1] = "全武器の攻撃力を変更\n(Dword)"}, {[1] = ""})
								if val and val[1] ~= nil then
									gg.editAll(val[1], gg.TYPE_DWORD)									
									break
								else
									temporarilyhide()
								end
							end
							WeaponLevelSpecSettingsProto_Attack()
						end
					elseif WeaponLevelSpecSettingsProtoAttackMenu == 5 then
						WeaponSpec()
					end
				end		
