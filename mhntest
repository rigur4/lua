				WeaponSpecSettingsProtoNormalSkillsetting_Selected = {false, false, false, false, false, false, false, false, false}
				function WeaponSpecSettingsProto_NormalSkillsetting()
					gg.clearResults()
					local results = gg.getListItems()
					local filteredResults = {}
					for _, v in ipairs(results) do
						if v.name == "WeaponSpecSettingsProto(0x0)" then
							table.insert(filteredResults, v)
						end
					end
					gg.loadResults(filteredResults)
					local offset = {0x18, 0x20, 0x10, 0x20, 0x10}
        			local function applyoffset(selectedOffset)
        				local results = gg.getResults(9999)
        				gg.clearResults()
        				for i, v in ipairs(results) do
        					v.address = v.address + selectedOffset
        					v.flags = gg.TYPE_DWORD
        				end
        			    gg.loadResults(results)
        			end							
					local function refine()
						local ranges = {{"5000~5503"},{"7000~7103"}} -- SPスキル以外になるようリファインする
						gg.getResults(9999)
						for _, rangeList in ipairs(ranges) do
							for _, range in ipairs(rangeList) do
								gg.getResults(9999)
								gg.refineNumber(range, gg.TYPE_DWORD, false, gg.SIGN_NOT_EQUAL, 0, -1, 0)
							end
						end
						gg.getResults(9999)
						gg.refineNumber("0~10,000", gg.TYPE_DWORD, false, gg.SIGN_EQUAL, 0, -1, 0)
					end
					gg.setVisible(false)
					local WeaponSelectNormalSkillMenu = gg.choice({
						(WeaponSpecSettingsProtoNormalSkillsetting_Selected[1] and "-> [1]    全武器 (一括変更)" or "[1]    全武器 (一括変更)"),
						(WeaponSpecSettingsProtoNormalSkillsetting_Selected[2] and "-> [2]    大剣" or "[2]    大剣"),
						(WeaponSpecSettingsProtoNormalSkillsetting_Selected[3] and "-> [3]    片手剣" or "[3]    片手剣"),
						(WeaponSpecSettingsProtoNormalSkillsetting_Selected[4] and "-> [4]    双剣" or "[4]    双剣"),
						(WeaponSpecSettingsProtoNormalSkillsetting_Selected[5] and "-> [5]    太刀" or "[5]    太刀"),
						(WeaponSpecSettingsProtoNormalSkillsetting_Selected[6] and "-> [6]    ハンマー" or "[6]    ハンマー"),
						(WeaponSpecSettingsProtoNormalSkillsetting_Selected[7] and "-> [7]    ランス" or "[7]    ランス"),
						(WeaponSpecSettingsProtoNormalSkillsetting_Selected[8] and "-> [8]    弓" or "[8]    弓"),
						(WeaponSpecSettingsProtoNormalSkillsetting_Selected[9] and "-> [9]    ライトボウガン" or "[9]    ライトボウガン"),
						"\n[←]    もどる"
					}, nil, "メインメニュー/装備/武器/全武器の性能/\n\nノーマルスキル (武器の選択)")
					if WeaponSelectNormalSkillMenu == nil then
						temporarilyhide()
						WeaponSpecSettingsProto_NormalSkillsetting()
					elseif WeaponSelectNormalSkillMenu >= 1 and WeaponSelectNormalSkillMenu <= 9 then	
						WeaponSpecSettingsProtoNormalSkillsetting_Selected[WeaponSelectNormalSkillMenu] = true	
						if WeaponSelectNormalSkillMenu == 1 then				
							WeaponSpecSettingsProtoNormalSkillsetting_Selected = {true, false, false, false, false, false, false, false, false}
							applyoffset(offset[1])
							applyoffset(offset[2])
							MoveThePointer()
							applyoffset(offset[3])
							MoveThePointer()
							applyoffset(offset[4])
							MoveThePointer()
							applyoffset(offset[5])
							refine()
							WeaponSpecSettingsProto_NormalSkillAll()                            
						elseif WeaponSelectNormalSkillMenu == 2 then
							WeaponSpecSettingsProtoNormalSkillsetting_Selected[1] = false
							applyoffset(offset[1])
							gg.refineNumber("1", gg.TYPE_DWORD, false, gg.SIGN_EQUAL, 0, -1, 0)
							applyoffset(offset[2])
							MoveThePointer()
							applyoffset(offset[3])
							MoveThePointer()
							applyoffset(offset[4])
							MoveThePointer()
							applyoffset(offset[5])
							refine()
							WeaponSpecSettingsProto_NormalSkillGreatSword()
						elseif WeaponSelectNormalSkillMenu == 3 then
							WeaponSpecSettingsProtoNormalSkillsetting_Selected[1] = false
							applyoffset(offset[1])
							gg.refineNumber("2", gg.TYPE_DWORD, false, gg.SIGN_EQUAL, 0, -1, 0)
							applyoffset(offset[2])
							MoveThePointer()
							applyoffset(offset[3])
							MoveThePointer()
							applyoffset(offset[4])
							MoveThePointer()
							applyoffset(offset[5])
							refine()
							WeaponSpecSettingsProto_NormalSkillSwordShield()								
						elseif WeaponSelectNormalSkillMenu == 4 then
							WeaponSpecSettingsProtoNormalSkillsetting_Selected[1] = false
							applyoffset(offset[1])
							gg.refineNumber("3", gg.TYPE_DWORD, false, gg.SIGN_EQUAL, 0, -1, 0)
