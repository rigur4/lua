						temporarilyhide()
						gg.setVisible(false)
						local choice = gg.alert("メインメニュー/装備/生産・強化\n\n防具の強化に必要な素材の確認\n", "[→]    変更し直す", "[←]    もどる")
    					if choice == 1 then
    						ArmorCraftSharedSettingsProto_CraftArmorMaterial()
