					elseif HunterInBattleInvicibleTimeMenu == 3 then
						while true do
							gg.setVisible(false)
							val = gg.prompt({[1] = "„Éè„É≥„Çø„ÉºÂæ©Ê¥ªÊôÇ„ÅÆÁÑ°ÊïµÊôÇÈñì„ÇíÂÖ•Âäõ\n(Áßí[s],„Éá„Éï„Ç©:2.5,Float)"}, {[1] = ""})
