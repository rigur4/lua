--	„É°„Ç§„É≥„É°„Éã„É•„Éº/	„Éû„ÉÉ„Éó	--
	function Map()
		gg.clearList()
		gg.clearResults()
		gg.setVisible(false)
		MapMenu = gg.choice({
		"[1]    Ë™≠„ÅøËæº„ÅøÁØÑÂõ≤",
		"[2]    „Éè„É≥„Çø„Éº„Ç¢„ÇØ„Çª„ÇπÁØÑÂõ≤",
		"[3]    „Ç™„Éà„É¢„Ç¢„ÇØ„Çª„ÇπÁØÑÂõ≤",
		"[4]    „Éñ„Éº„Çπ„ÉàÊôÇ„ÅÆ„Ç¢„ÇØ„Çª„ÇπÁØÑÂõ≤",
		"[‚Üê]    „ÇÇ„Å©„Çã"
		}, nil,"„É°„Ç§„É≥„É°„Éã„É•„Éº/„Éû„ÉÉ„Éó")
		if MapMenu == nil then
			temporarilyhide()
			Map()
		else
			if MapMenu == 1 then
				gg.setVisible(false)
				valueFromClass("MapViewport","0x30", false, false, gg.TYPE_DOUBLE)
				gg.getResults(9999)
				gg.refineNumber("0~100,000", gg.TYPE_DOUBLE, false, gg.SIGN_EQUAL, 0, -1, 0)			
				Map_LoadingRange()
			elseif MapMenu == 2 then 
				gg.setVisible(false)
				valueFromClass("DistanceSensor","0x3C", false, false, gg.TYPE_FLOAT)
				Map_HunterAccessRange()
			elseif MapMenu == 3 then
				gg.setVisible(false)
				valueFromClass("DistanceLayerInfo","0x10", false, false, gg.TYPE_FLOAT)
				Map_OtomoAccessRange()
			elseif MapMenu == 4 then
				gg.setVisible(false)
				valueFromClass("FieldConfig","0x20", false, false, gg.TYPE_FLOAT)			
				Map_BoostAccessRange()
			elseif MapMenu == 5 then
				mainMenu()
			end
		end
	end
	--	„É°„Ç§„É≥„É°„Éã„É•„Éº/	„Éû„ÉÉ„Éó/	Ë™≠„ÅøËæº„ÅøÁØÑÂõ≤	--
		function Map_LoadingRange()
			gg.getResults(9999)
			gg.setVisible(false)
			local MapLoadingRange = {"350", "700"}
			MapLoadingRangeMenu = gg.choice({
			"[1]    „Éá„Éï„Ç©„É´„Éà",
			"[2]    2ÂÄç",
			"[3]    ‰ªªÊÑè„ÅÆÂÄ§",
			"[‚Üê]    „ÇÇ„Å©„Çã"
			},nil,"„É°„Ç§„É≥„É°„Éã„É•„Éº/„Éû„ÉÉ„Éó/Ë™≠„ÅøËæº„ÅøÁØÑÂõ≤")
			if MapLoadingRangeMenu == nil then
				temporarilyhide()
				Map_LoadingRange()
			else
				if MapLoadingRangeMenu >= 1 and MapLoadingRangeMenu <= 2 then
					local chosenValue = MapLoadingRange[MapLoadingRangeMenu]
					gg.editAll(chosenValue, gg.TYPE_DOUBLE)
					Map_LoadingRange()
				elseif MapLoadingRangeMenu == 3 then
					while true do
						gg.setVisible(false)
    					val = gg.prompt({[1] = "„Éû„ÉÉ„ÉóË™≠„ÅøËæº„ÅøÁØÑÂõ≤„ÇíÂÖ•Âäõ\n(ÂçäÂæÑ[m],„Éá„Éï„Ç©:350,Double)"}, {[1] = ""})
    					if val and val[1] ~= nil then
    						gg.editAll(val[1], gg.TYPE_DOUBLE)
							break
						else
							temporarilyhide()
						end
					end				
					Map_LoadingRange()
				elseif MapLoadingRangeMenu == 4 then
					Map()
				end
			end		
		end
--	„É°„Ç§„É≥„É°„Éã„É•„Éº/	„Éû„ÉÉ„Éó/	„Éè„É≥„Çø„Éº„Ç¢„ÇØ„Çª„ÇπÁØÑÂõ≤	--
