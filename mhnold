								gg.editAll("1", gg.TYPE_DWORD)
							elseif MonsterWeaknesElementTypePoisonMenu == 2 then
								gg.editAll("0", gg.TYPE_DWORD)
							end
							break
						end
					end
					Monster_WeaknesElementType()
				elseif MonsterWeaknesElementTypeMenu == 7 then	
