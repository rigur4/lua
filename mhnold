								gg.editAll(val[1], gg.TYPE_DWORD)
								break
							else
								temporarilyhide()
							end
						end
						Equipment_LongSwordLogic_MaxSpirits()
					elseif EquipmentLongSwordLogicMaxSpiritsMenu == 3 then
						Equipment_LongSwordLogic()
					end
				end
			end
